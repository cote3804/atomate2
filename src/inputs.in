converge-empty-states yes 

lattice-minimize \
    nIterations 0  
electronic-minimize \
    nIterations 100 \
    energyDiffThreshold 1e-07  

dump End Dtot  
dump End BoundCharge  
dump End State  
dump End Forces  
dump End Ecomponents  
dump End VfluidTot  
dump End ElecDensity  
dump End KEdensity  
dump End EigStats  
dump End BandEigs  
dump End bandProjections  
dump End DOS  
band-projection-params yes no  

